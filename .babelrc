{
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "@/": "./src"
        }
      }
    ]
  ],
  "presets": [
    "@babel/preset-typescript",
    [
      "@babel/preset-env",
      {
        "targets": {
          "browserslist": "> 0.25%, not dead",
          "useBuiltIns": "usage"
        }
      }
    ]
  ]
}
